{
    "id": "go-nats",
    "version": "0.0.2",
    "name": "Go & NATS.io",
    "description": "Use and develop Go + NATS.io. Includes appropriate runtime args, Go, common tools, extensions, and dependencies.",
    "documentationURL": "https://github.com/kmpm/my-devcontainers/tree/main/src/go-nats",
    "licenseURL": "https://github.com/kmpm/my-devcontainers/tree/main/LICENSE",
    "options": {
        "imageVariant": {
            "type": "string",
            "description": "Go version:",
            "proposals": [
                "1-bookworm",
				"1.23-bookworm",
                "1.22-bookworm",
                "1-bullseye",
				"1.23-bullseye",
                "1.22-bullseye"
            ],
            "default": "1.23-bookworm"
        },
		"natsVariant": {
            "type": "string",
            "description": "Version of nats server",
            "proposals": [
				"latest",
                "2-scratch",
				"2-alpine",
                "2.10-scratch",
				"2.10-alpine",
                "2",
				"2.10"
            ],
            "default": "2.10"
        }
    },
    "platforms": ["Go"],
    "optionalPaths": [
        ".github/*"
    ]
}